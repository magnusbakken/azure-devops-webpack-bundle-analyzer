{
    "manifestVersion": 1,
    "id": "generate-webpack-bundle-analyzer-report-task",
    "publisher": "MagnusGrindalBakken",
    "version": "0.2.0",
    "name": "Generate webpack-bundle-analyzer Report",
    "description": "Generate a report from webpack-bundle-analyzer using an existing stats.json file.",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "icon.png"        
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "contributions": [
        {
            "id": "generate-wba-report-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}